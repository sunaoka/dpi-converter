{
    "name": "sunaoka/dpi-converter",
    "type": "library",
    "description": "",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.69",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.0"
    },
    "autoload": {
        "psr-4": {
